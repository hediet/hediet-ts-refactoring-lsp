{
	"name": "hediet-ts-refactoring-lsp",
	"displayName": "Hediet TypeScript Refactoring Language Service Plugin",
	"description": "This is a VS Code Extension for refactorings that I had time and fun to program.",
	"version": "0.1.0",
	"license": "MIT",
	"engines": {
		"vscode": "^1.30.0"
	},
	"publisher": "hediet",
	"author": {
		"email": "henning.dieterichs@live.de",
		"name": "Henning Dieterichs"
	},
	"readme": "../README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/hediet/hediet-ts-refactoring-lsp.git"
	},
	"categories": [
		"Other"
	],
	"contributes": {
		"typescriptServerPlugins": [
			{
				"enableForWorkspaceTypeScriptVersions": true,
				"name": "@hediet/ts-refactoring-lsp"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run build",
		"build": "tsc -p ./",
		"dev": "tsc -watch -p ./",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"dependencies": {
		"@hediet/ts-refactoring-lsp": "0.1.1"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.6.3",
		"tslint": "^5.18.0",
		"typescript": "^3.5.3",
		"@types/vscode": "^1.36.0"
	}
}
